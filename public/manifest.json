{
  "name": "Web chat app",
  "short_name": "Web chat",
  "start_url": "/",
  "scope": "/",
  "description": "High aspiration web chat",
  "display": "standalone",
  "orientation": "portrait",
  "background_color": "#80b080",
  "theme_color": "#80b080",
  "share_target": {
    "action": "/share-target?share-target",
    "method": "POST",
    "enctype": "multipart/form-data",
    "params": {
      "title": "name",
      "text": "description",
      "url": "link",
      "files": [
        {
          "name": "image",
          "accept": "image/*"
        }
      ]
    }
  },
  "icons": [
    {
      "src": "/test-72x72.png",
      "sizes": "72x72 48x48 96x96",
      "type": "image/png"
    },
    {
      "src": "/test-144x144.png",
      "sizes": "144x144 256x256 512x512",
      "type": "image/png"
    }
  ]
}
